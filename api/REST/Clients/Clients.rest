# Create Client with Contact Body and Address Body
POST http://localhost:5000/api/clients
Content-Type: application/json

{
  "name": "Client A",
  "contact": {
    "name": "John Doe",
    "phone": "+1234567890",
    "email": "johndoe@example.com"
  },
  "address": {
    "street": "Main Street",
    "house_number": "10",
    "zipcode": "12345",
    "city": "Metropolis",
    "province": "Central",
    "country_code": "US"
  }
}
